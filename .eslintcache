 price = param[1];\n            }\n            else {\n                ingredients[param[0]] = +param[1];\n            }\n        }\n        this.setState({ingredients:ingredients,totalPrice:price});\n    }\n\n    render() {\n        return(\n            <div>\n                <CheckoutSummary \n                onCheckoutCancelled={this.checkoutCancelHandeler}\n                onCheckoutContinued={this.checkoutContinueHandler}\n                ingredients={this.state.ingredients}/>\n                <Route path={this.props.match.path + '/contact-data'}\n                    render={(props) => (<ContactData ingredients={this.state.ingredients} price={this.state.totalPrice} {...props}/>)}/>\n            </div>\n        );\n    }\n}\n\nexport default Checkout;","/home/ashu12_chi/React/burger-builder/src/Components/Order/CheckoutSummary/CheckoutSummary.js",[],"/home/ashu12_chi/React/burger-builder/src/Containers/Checkout/ContactData/ContactData.js",[],"/home/ashu12_chi/React/burger-builder/src/Containers/Orders/Orders.js",[],"/home/ashu12_chi/React/burger-builder/src/Components/Order/Order.js",[],"/home/ashu12_chi/React/burger-builder/src/Components/UI/Input/Input.js",[],{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","severity":1,"message":"134","line":1,"column":16,"nodeType":"135","messageId":"136","endLine":1,"endColumn":25},{"ruleId":"137","severity":1,"message":"138","line":65,"column":21,"nodeType":"139","messageId":"140","endLine":65,"endColumn":23},{"ruleId":"137","severity":1,"message":"138","line":13,"column":37,"nodeType":"139","messageId":"140","endLine":13,"endColumn":39},{"ruleId":"137","severity":1,"message":"138","line":31,"column":26,"nodeType":"139","messageId":"140","endLine":31,"endColumn":28},"no-native-reassign",["141"],"no-negated-in-lhs",["142"],"no-unused-vars","'Component' is defined but never used.","Identifier","unusedVar","eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","n